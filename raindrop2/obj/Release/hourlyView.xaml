                                    <TextBlock x:Name="hourHead" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="18" Margin="0,0,0,0" FontFamily="Segoe UI Semibold" Foreground="#FFFFBB33"/>
                                </StackPanel>
                            </Border>
