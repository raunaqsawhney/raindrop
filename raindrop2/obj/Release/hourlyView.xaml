                                <Image x:Name="weatherIcon" Width="66" Height="66" Margin="0,5,0,0"/>
                                <StackPanel Width="200">
                                    <TextBlock x:Name="temp" TextWrapping="Wrap" FontSize="32" Margin="10,0" FontFamily="{StaticResource MainUIFont}"/>
                                    <TextBlock x:Name="desc" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="16" Margin="10,0" FontFamily="{StaticResource MainUIFont}"/>
                                </StackPanel>
                                <Border x:Name="border" BorderBrush="#FFFFBB33" BorderThickness="1,0,0,0" RenderTransformOrigin="0.5,0.5" Margin="0" />
                                <StackPanel x:Name="stackPanel" Width="127" Orientation="Horizontal" Margin="10,5,0,0">
                                    <StackPanel x:Name="Left" Orientation="Vertical">
                                        <StackPanel Orientation="Horizontal">
                                            <Image x:Name="rainIcon" Source="Assets/Icons/pop.png" Width="16" Height="16" ></Image>
                                            <TextBlock x:Name="rainData" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="14" FontFamily="{StaticResource MainUIFont}" Margin="10,0,0,0"/>
                                        </StackPanel>
